  "name": "notion-add-task",
  "version": "1.0.0",
  "description": "Adds a task to Notion with optional due date parsing (supports today, tomorrow, day names, and specific dates).",
  "category": "productivity",
  "voice_triggers": [
    "Add task buy groceries",
    "Add task call mom due tomorrow"
  ],
  "required_services": [
    "notion"
  ],
  "required_credentials": [
    {
      "credential_type": "notionApi",
      "name": "NOTIONAPI_CREDENTIAL",
      "description": "n8n credential type: notionApi"
    }
  ],
  "required_variables": [
    {
      "name": "NOTION_DATABASE_ID",
      "description": "Your Notion task database ID",
      "example": "abc123def456789xyz"
    }
  ],
